    <script>

        function alerta(titulo, mensaje, tipo) {
            $.notify({
                title: titulo,
                message: mensaje
            },
                {
                    type: tipo,
                    allow_dismiss: true,
                    newest_on_top: false,
                    mouse_over: true,
                    showProgressbar: false,
                    spacing: 10,
                    timer: 2000,
                    placement: {
                        from: 'top',
                        align: 'center'
                    },
                    offset: {
                        x: 30,
                        y: 30
                    },
                    delay: 1000,
                    z_index: 10000,
                    animate: {
                        enter: 'animated bounce',
                        exit: 'animated bounce'
                    }
                });
        }
    </script>

    <script>
        var logo = "data:image/png;base64,";
        const addHeaderReport = ({ width }, { titulo, nombreArchivo, codigoReporte = "  " }) => {
            var encabezado = [];
            let iso1 = {
                columns: [
                    { text: codigoReporte, fontSize: 10, alignment: 'right' }
                ],
                margin: [0, 10, 30, 0]
            };
            let iso2 = {
                columns: [
                    { text: ' ', fontSize: 10, alignment: 'right' }
                ],
                margin: [0, 0, 30, 0]
            };
            let iso3 = {
                columns: [
                    { text: ' ', fontSize: 10, alignment: 'right' }
                ],
                margin: [0, 0, 30, 0]
            };
            let headLogo = {
                image: logo,
                width: 40,
                height: 40,
                margin: [30, -15, 0, 0]
            };
            let headTitulo = {
                text: "Mis Compras",
                style: "title",
                bold: true,
                fontSize: 15,
                margin: [0, -45, 0, 0]
            }
            let headTtituloPrincial = {
                text: titulo,
                style: "title",
                fontSize: 12,
                margin: [0, 0, 0, 0]
            };
            let headReportName = {
                text: nombreArchivo,
                bold: false,
                style: "title",
                fontSize: 15,
                margin: [0, 10, 0, 0]
            };
            let headLine = { canvas: [{ type: 'line', x1: 30, y1: 0, x2: (width - 30), y2: 0, lineWidth: 1 }] };
            encabezado.push(iso1);
            encabezado.push(iso2);
            encabezado.push(iso3);
            encabezado.push(headLogo);
            encabezado.push(headTitulo);
            encabezado.push(headTtituloPrincial);
            encabezado.push(headReportName);
            encabezado.push(headLine);
            return encabezado;
        }
        const addFootherReport = (page, pages, { width }, { nombreUsuario }) => {
            let fechaHoy = new Date();
            let mes = fechaHoy.getMonth() + 1;
            let dia = fechaHoy.getDate();
            let hora = fechaHoy.getHours();
            let minuto = fechaHoy.getMinutes();
            let segundos = fechaHoy.getSeconds();
            if (mes < 10) {
                mes = "0" + mes;
            }
            if (dia < 10) {
                dia = "0" + dia;
            }
            if (hora < 10) {
                hora = "0" + hora;
            }
            if (minuto < 10) {
                minuto = "0" + minuto;
            }
            if (segundos < 10) {
                segundos = "0" + segundos;
            }
            let fechaTexto = `${dia}/${mes}/${fechaHoy.getFullYear()} ${hora}:${minuto}:${segundos}`;
            let footer = [];
            let item1 = {
                columns: [
                    {
                        alignment: 'left',
                        text: "Generado por: " + nombreUsuario + " - fecha: " + fechaTexto,
                        fontSize: 10
                    },
                    {
                        alignment: 'right',
                        text: ['Página ', { text: page.toString() }, ' de ', { text: pages.toString() }],
                        fontSize: 10,
                    }
                ],
                margin: [30, 0]
            };

            let footLine = { canvas: [{ type: 'line', x1: 30, y1: 0, x2: (width - 30), y2: 0, lineWidth: 0.5 }] };

            footer.push(footLine);
            footer.push(item1);

            return footer;
        }
        const PersonaLisarReporte = (configuracion) => {
            let { pageSize = "LETTER", titulo, nombreUsuario = "", nombreArchivo = 'Reporte', selector, indiceColumnaOrden = 0, order = 'asc', totalFilasMostrar = 25, btnTituloPdf = 'Exportar a Pdf', orientacion = 'landscape', customizarReporte } = !!configuracion && configuracion;
            var info = {
                title: titulo,
                author: nombreUsuario,
                subject: '',
                keywords: '',
                creator: 'Mis Compras',
                producer: 'Mis Compras'
            };

            $(selector).DataTable({
                language: {
                    processing: "Cargando información",
                    search: "Filtrar por:",
                    lengthMenu: "Mostrar _MENU_ filas",
                    info: "Vizualización  _END_ de _TOTAL_ filas",
                    infoEmpty: "Vizualización del elemento 0 a 0 de 0 filas",
                    infoFiltered: "(Filtrado de _MAX_ filas en total)",
                    infoPostFix: "",
                    loadingRecords: "Cargando...",
                    zeroRecords: "No se logró encontrar ninguna coincidencia",
                    emptyTable: "No existen registros",
                    paginate: {
                        first: "Primera",
                        previous: "Anterior",
                        next: "Siguiente",
                        last: "Ultima"
                    },
                    aria: {
                        sortAscending: ": active para ordenar la columna en orden ascendente",
                        sortDescending: ": active para ordenar la columna en orden descendente"
                    }
                },
                order: [[indiceColumnaOrden, order]],
                displayLength: totalFilasMostrar,
                dom: 'Bfrtip',
                buttons: [
                    {
                        extend: 'pdfHtml5',
                        text: btnTituloPdf,
                        orientation: orientacion,
                        title: '',
                        pageSize: pageSize,
                        className: 'btn btn-info mr-1',
                        filename: titulo,
                        customize: (doc) => {
                            doc.info = info;
                            doc.pageMargins = [30, 90, 30, 70];
                            doc.defaultStyle = {
                                fontSize: 8
                            };

                            doc['footer'] = (function (page, pages, pageSize) {
                                return addFootherReport(page, pages, pageSize, configuracion);
                            });

                            doc['header'] = (function (currentPage, pageCount, pageSize) {
                                return addHeaderReport(pageSize, configuracion);
                            });

                            if (customizarReporte) {
                                customizarReporte(doc, configuracion);
                            }
                        }
                    },
                    {
                        extend: 'excelHtml5',
                        text: 'Exportar a Excel',
                        title: `${nombreArchivo} ${titulo}`,
                        className: 'btn btn-success',
                        filename: titulo,
                    }
                ]
            });
        }
    </script>

